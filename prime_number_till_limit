// Description:Write a program to find prime numbers within a limit using loops
//Author:Angelina Rose Varghese
//Date:09/02/2025

#include <stdio.h>
int main() 
{
   int result,limit,i;
   printf("Enter a number till which you want to find prime numbers: ");
   scanf("%d",&limit);
   if (limit>0)
   {
       for(i=1;i<=limit;i++)
       {
           if(i==2)
           {
               printf("%d\n",i);
           }
           if(i==3)
           {
               printf("%d\n",i);
           }
           if(i==5)
           {
               printf("%d\n",i);
           }
           if(i==7)
           {
               printf("%d\n",i);
           }
           if ((i%2!=0)&&(i%3!=0)&&(i%5!=0)&&(i%7!=0))
           {
               result=i;
               printf("%d\n",result);
           }
       }
       
   }
   else
   {
       printf("Invalid");
   }
    return 0;
}
