// Desription: To find largest number moung 3 numbers using nested if
#include<stdio.h>
int main()
{
    int num1,num2,num3;
    printf("Enter 3 numbers: ");
    scanf("%d%d%d",&num1,&num2,&num3);
    if (num1>num2) 
        if (num1>num3)
            {
            printf("%d number is largest/n",num1);
            }
        else 
            {
            printf("%d number is largest/n",num3);
            }
    else
        if (num2>num3)
            {
            printf("%d number is largest/n",num2);
            }
        else
            {
            printf("%d number is largest/n",num3);
            }
    return 0;
}
